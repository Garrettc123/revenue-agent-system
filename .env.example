# Revenue Agent System - Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Payment Processing
PAYMENT_ACCOUNT=your_payment_account_here

# API Security
API_TOKEN=your_secure_api_token_here

# Database Configuration
POSTGRES_DB=revenue_agent
POSTGRES_USER=agent
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Grafana Admin
GF_SECURITY_ADMIN_PASSWORD=your_secure_grafana_password_here
GF_SERVER_ROOT_URL=http://localhost:3000

# Trading Configuration
TICKERS=AAPL,TSLA,MSFT,NVDA,GOOGL,AMZN

# Redis Configuration (optional)
REDIS_PASSWORD=your_secure_redis_password_here
